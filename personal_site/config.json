{
    "global": {
        "title": "Title for all of templates",
        "language": "zh-cn",
        "copyright": "Company, Inc.",
        "jumbotron": {
            "fullwidth": true,
            "alignment": "left",
            "tag": "div",
            "_comment_": {
                "fullwidth": "occupies the entire horizontal space of its parent.",
                "tag": "div, section",
                "alignment": "justify, left, right, center"
            }
        },
        "popover": {
            "mode": "toggle",
            "direction": "right",
            "tag": "button",
            "_comment_": {
                "mode": "toggle, dismiss",
                "direction": "left, right, top, bottom",
                "tag": "a, button"
            }
        },
        "breadcrumb": {
            "crumbs": [{
                "title": "Home",
                "link": "/"
            },{
                "title": "Library",
                "link": "/library"
            },{
                "title": "Data",
                "link": "/library/data"
            }]
        },
        "carousel": {
            "id_prefix": "carousel",
            "mode": "slide",
            "controls": true,
            "indicators": true,
            "slides": [{
                "image": "holder.js/800x400?auto=yes&bg=777&fg=555&text=First slide",
                "caption": {
                    "title": "First Slide",
                    "description": "first slide description"
                }
            },{
                "image": "holder.js/800x400?auto=yes&bg=666&fg=444&text=Second slide",
                "caption": {
                    "title": "Second Slide",
                    "description": "second slide description"
                }
            },{
                "image": "holder.js/800x400?auto=yes&bg=555&fg=333&text=Third slide",
                "caption": {
                    "title": "Third Slide",
                    "description": "third slide description"
                }
            }],
            "_comment_": {
                "mode": "slide, fade"
            }
        },
        "card": {
            "sizing": "w-75",
            "style": "",
            "alignment": "left",
            "color": "dark",
            "border_color": "warning",
            "bg_color": "primary",
            "img_top": {
                "image": "holder.js/100px180/?text=Image top cap",
                "title": "Image Top Cap"
            },
            "img_bottom": {
                "image": "holder.js/100px180/?text=Image bottom cap",
                "title": "Image Bottom Cap"
            },
            "header": {
                "title": "Card Header",
                "alignment": "right",
                "color": "white",
                "border_color": "warning",
                "bg_color": "dark"
            },
            "footer": {
                "title": "Card Footer",
                "alignment": "center",
                "color": "secondary",
                "border_color": "danger",
                "bg_color": "primary"
            },
            "body": {
                "alignment": "left",
                "color": "dark",
                "border_color": "warning",
                "bg_color": "transparent"
            },
            "_comment_": {
                "color": "text-* classes: primary, secondary, dark, light, white, muted, success, error, warning, danger, info",
                "border_color": "border-* classes: primary, secondary, dark, light, success, error, warning, danger, info",
                "bg_color": "bg-* classes: primary, secondary, dark, light, transparent, success, error, warning, danger, info",
                "alignment": "justify, left, right, center",
                "sizing": "sizing classes",
                "style": "add style attribution for card element, e.g., width: 18rem;"
            }
        },
        "tooltip": {
            "tag": "a",
            "direction": "top",
            "trigger": "hover focus",
            "_comment_": {
                "tag": "a, button",
                "direction": "left, right, top, bottom",
                "trigger": "hover, focus, click"
            }
        },
        "codesnippet": {
            "pre_class": "code-snippet",
            "clipboard_trigger_class": "clipboard-trigger",
            "clipboard_enable_attrname": "data-clipboard-snippet",
            "clipboard_button_svg": "/img/clippy.svg"
        },
        "nav": {
            "mode": "pill",
            "tag": "ul",
            "direction": "horizontal",
            "alignment": "right",
            "extend": "justified",
            "breakpoint": "sm",
            "_comment_": {
                "mode": ", tab, pill",
                "tag": "ul",
                "direction": ", vertical, horizontal",
                "alignment": ", left, right, center",
                "extend": ", fill, justified",
                "breakpoint": ", xs, sm, md, lg, xl"
            }
        },
        "navtab": {
            "id_prefix": "navtab",
            "mode": "tab",
            "direction": "horizontal",
            "alignment": "right",
            "extend": "justified",
            "tag": "ul",
            "_comment_": {
                "mode": "tab, pill",
                "direction": ", vertical, horizontal",
                "alignment": ", left, right, center",
                "extend": ", fill, justified",
                "tag": "ul, nav"
            }
        },
        "navbar": {
            "id_prefix": "navbar",
            "outer_container": true,
            "inner_container": false,
            "rounded": false,
            "position": "top",
            "bg_mode": "dark",
            "bg_color": "dark",
            "style": "",
            "breakpoint": "md",
            "collapsed": true,
            "toggler_alignment": "right",
            "text": "Navbar text with an inline element",
            "brand": {
                "title": "Navbar",
                "link": "#",
                "image": {
                    "link": "/img/bootstrap-solid.svg",
                    "alt": "Brand Image"
                }
            },
            "nav": {
                "tag": "ul",
                "_comment_": {
                    "tag": "ul, div"
                }
            },
            "items": [{
                "title": "Home",
                "link": "/"
            },{
                "title": "Gallery - card",
                "link": "/gallery-card.html"
            },{
                "title": "Gallery - thumbnail",
                "link": "/gallery-thumbnail.html"
            }],
            "_comment_": {
                "position": "top, bottom, stickied, static",
                "bg-mode": "dark, light",
                "bg-color": "bg-* classes: dark, white, primary,,,",
                "style": "custom css for navbar, e.g., background-color: #e3f2fd;",
                "breakpoint": ", sm, md, lg, xl",
                "collapsed": "if breakpoint is empty, collapsed=true means always collapse, collapsed=false means never collapse",
                "brand": "add title or image, yeah you can add both of them",
                "items": "conflict with items(), please using ['items'] in template",
                "outer_container": "wrap a navbar in a .container to center it on a page",
                "inner_container": " add a .container within to only center the contents of a fixed or static top navbar.",
                "toggler_alignment": "left, right, inner; how to align brand and responsive toggler button"
            }
        },
        "footer": {
            "container": true,
            "border_top": true,
            "color": "muted"
        },
        "progressbar": {
            "value": "75",
            "width": "75%",
            "height": "20px",
            "label": "75%",
            "background": "primary",
            "striped": true,
            "animated": true
        }
    },
    "local": {
        "gallery": {
            "title": "Title for gallery",
            "jumbotron": {
                "fullwidth": true,
                "alignment": "center",
                "tag": "section"
            },
            "navbar": {
                "position": "static"
            }
        }
    }
}